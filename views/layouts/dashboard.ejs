            if (prefix.length === arrays.length) {
                results.push(prefix.slice());
                return;
            }

            for (let i = 0; i < arrays[prefix.length].length; i++) {
                prefix.push(arrays[prefix.length][i]);
                getCombinations(arrays, prefix, results);
                prefix.pop();
            }

            return results;
        }

